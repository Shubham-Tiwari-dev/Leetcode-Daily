class Solution {
public:
    bool isprime(int count){
        if(count<2) return false;
        for(int i=2;i*i<=count;i++)
            if(count%i==0) return false;
        return true;
    }
    int countPrimeSetBits(int left, int right) {
        int count=0, ans=0;
        for(int i=left;i<=right;i++){
            int n=i,count=0;
            while(n){
                n=n & (n-1);
                count++;
            }
            if(isprime(count)) ans++;
        }
        return ans;
    }
};
